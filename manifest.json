{
  "manifest_version": 3,
  "name": "Import UMD Schedule to Google Calendar",
  "description": "Base Level Extension",
  "version": "1.0",
  "key": "####################################",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/testudo.png"
  },
  "permissions": ["tabs", "activeTab", "scripting", "identity"],
  "content_scripts": [
    {
      "matches": ["https://*.testudo.umd.edu/*"],
      "runAt": "document_idle",
      "js": ["schedule-importer.js"]
    }
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "oauth2": {
    "client_id": "####################################",
    "scopes": ["https://www.googleapis.com/auth/calendar"]
  }
}
